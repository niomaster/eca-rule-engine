IMPORT standaard
IMPORT imports

CONSTANT re = module("re")
DECLARE ptrn = re.compile('((https?:\/\/[-a-z0-9.]+|www\.[-a-z0-9.]+)[^\s<>"]+)', re.I)

CELL 'tweetlistcell_urls' {'event': 'createTweetlistGadget', 'data': [json_serialize({"cell": "tweetlistcell_urls", "id": "myUrlTweetlist", "title": "tweets"})]}

RULE: new_tweet_rule
EVENT: new_tweet
CONDITION: len(ptrn.findall(new_tweet.text)) > 0
ACTION:	add_tweetlist_tweet('myUrlTweetlist', new_tweet.json_dict)
	#print(new_tweet.id + "")


RULE: the_finalization_rule
EVENT: finalize
CONDITION: True
ACTION:	print("done")
